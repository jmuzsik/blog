{
  "version": "3.0.0",
  "name": "blog",
  "description": "A minimal, lightweight and mobile-first starter for creating blazing-fast static blogs",
  "main": "blog",
  "scripts": {
    "develop": "yarn run clean && gatsby develop",
    "build": "yarn run clean && gatsby build",
    "deploy": "yarn run clean && gatsby build && netlify deploy --prod",
    "clean": "rimraf .cache public",
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint:js": "eslint --cache --ext  .js,.jsx .",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:js\" \"yarn run lint:scss\" \"yarn flow\"",
    "test": "jest --config ./jest/jest-config.js",
    "test:coverage": "jest --coverage --config ./jest/jest-config.js",
    "test:watch": "jest --watch --config ./jest/jest-config.js",
    "release": "standard-version"
  },
  "repository": "https://github.com/alxshelepenok/gatsby-starter-lumen",
  "author": "Alexander Shelepenok <alxshelepenok@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "graphql",
    "netlify",
    "pagination",
    "categories",
    "tags",
    "prismjs",
    "static",
    "lost",
    "blog",
    "minimal",
    "react",
    "theme",
    "starter",
    "disqus"
  ],
  "dependencies": {
    "bluebird": "latest",
    "classnames": "latest",
    "codecov": "latest",
    "gatsby": "2.13.53",
    "gatsby-link": "2.2.4",
    "gatsby-plugin-catch-links": "2.1.2",
    "gatsby-plugin-feed": "2.3.6",
    "gatsby-plugin-flow": "1.1.2",
    "gatsby-plugin-manifest": "2.2.5",
    "gatsby-plugin-offline": "2.2.5",
    "gatsby-plugin-react-helmet": "3.1.3",
    "gatsby-plugin-sass": "2.1.4",
    "gatsby-plugin-sharp": "2.2.10",
    "gatsby-plugin-sitemap": "2.2.5",
    "gatsby-remark-autolink-headers": "2.1.3",
    "gatsby-remark-copy-linked-files": "2.1.4",
    "gatsby-remark-images": "3.1.8",
    "gatsby-remark-katex": "3.1.2",
    "gatsby-remark-prismjs": "3.3.5",
    "gatsby-remark-relative-images": "0.2.3",
    "gatsby-remark-responsive-iframe": "2.2.4",
    "gatsby-remark-smartypants": "2.1.2",
    "gatsby-source-filesystem": "2.1.9",
    "gatsby-transformer-remark": "2.6.11",
    "gatsby-transformer-sharp": "2.2.6",
    "invariant": "latest",
    "katex": "latest",
    "lodash": "4.17.15",
    "moment": "latest",
    "node-sass": "latest",
    "normalize-scss": "latest",
    "prismjs": "1.25.0",
    "react": "latest",
    "react-disqus-comments": "latest",
    "react-dom": "latest",
    "react-helmet": "latest"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-optional-chaining": "latest",
    "@babel/plugin-transform-flow-strip-types": "latest",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "latest",
    "autoprefixer": "9.6.1",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-jest": "latest",
    "babel-preset-gatsby": "0.2.8",
    "browserslist": "4.6.6",
    "concurrently": "latest",
    "eslint": "6.1.0",
    "eslint-config-airbnb-base": "latest",
